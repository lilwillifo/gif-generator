<main class='show'>
  <section>
    <h1>Welcome, <%= current_user.username %>!</h1>
    <h3>Your favorite gifs:</h3>
  </section>
  <div class='all_gifs'>
    <% current_user.categories.each do |category| %>
    <%= category.name %>    
    <% end %>
    <% current_user.gifs.each do |gif| %>
    <div id="img-<%=gif.id%>">
     <img src=<%= gif.image_path %> class='gif.image_path'>
     <%= link_to "<i class='far fa-times-circle'></i>".html_safe, gif_path(gif), method: 'put', title: 'Remove from Favorites', class: 'x' %>
    </div>
    <% end %>
  </div>
</main>
